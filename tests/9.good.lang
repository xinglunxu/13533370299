Main {

     main() -> none {
     	    integer i, x;
	    boolean cond;

	    i = 0;
	    x = 1;
	    cond = false;

	    while i <= 8 {
	    	  cond = 0 < i - (i / 2) * 2;
		  i = i + 1;
		  if cond {
		     x = x * 2;
		  } else {
		     x = x + 1;
		  }
		  print x;
	    }
     	    

     }    

}
