classA {
     integer x;

     classA(val : integer) -> none {
         x = val;
     }

     f0() -> none {
         x = x * 2;
     }
}

classB extends classA {
    boolean y;

    classB(val1 : integer, val2 : boolean) -> none {
        classA(val1);
	y = val2;
    }

}


Main {

     main() -> none {
	    classB b;

	    b = new classB(12, true);
	    print b.x;
	    print b.y;
	    b.f0();
	    print b.x;
	    print b.y;
     }

}
